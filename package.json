{
  "name": "ticker-importers-tv",
  "version": "1.0.0",
  "description": "YouTube video importer for Ticker TV with statistics tracking",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "import": "tsx src/fetch-youtube-videos.ts",
    "test": "tsx src/fetch-youtube-videos.ts --limit-videos 3",
    "export": "tsx src/export-to-csv.ts",
    "verify": "tsx src/verify-data.ts"
  },
  "keywords": ["youtube", "supabase", "typescript", "importer"],
  "author": "Ticker TV",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.48.0",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.0"
  }
}